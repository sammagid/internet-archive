{
    "title": "chat testing again",
    "steps": [
        {
            "type": "setViewport",
            "width": 994,
            "height": 981,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "navigate",
            "url": "https://chatgpt.com/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://chatgpt.com/",
                    "title": "ChatGPT"
                }
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "aria/[role=\"none\"]",
                    "aria/[role=\"paragraph\"]"
                ],
                [
                    "p"
                ],
                [
                    "xpath///*[@id=\"prompt-textarea\"]/p"
                ],
                [
                    "pierce/p"
                ]
            ],
            "offsetY": 8.203125,
            "offsetX": 171
        },
        {
            "type": "change",
            "value": "Who is George Washington?",
            "selectors": [
                [
                    "#prompt-textarea"
                ],
                [
                    "xpath///*[@id=\"prompt-textarea\"]"
                ],
                [
                    "pierce/#prompt-textarea"
                ]
            ],
            "target": "main"
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "aria/Send prompt",
                    "aria/[role=\"image\"]"
                ],
                [
                    "[data-testid='composer-trailing-actions'] svg"
                ],
                [
                    "xpath///*[@data-testid=\"send-button\"]/svg"
                ],
                [
                    "pierce/[data-testid='composer-trailing-actions'] svg"
                ]
            ],
            "offsetY": 11.703125,
            "offsetX": 8
        },
        {
            "type": "waitForElement",
            "target": "main",
            "selectors": [
                "[data-testid='conversation-turn-2'] [data-testid='copy-turn-action-button'] svg",
                "xpath///*[@data-testid=\"copy-turn-action-button\"]/span/svg",
                "pierce/[data-testid='conversation-turn-2'] [data-testid='copy-turn-action-button'] svg"
            ]
        }
    ]
}
