                [
                    "aria/Enter a prompt here"
                ],
                [
                    "div.ql-editor"
                ],
                [
                    "xpath///*[@id=\"app-root\"]/main/side-navigation-v2/mat-sidenav-container/mat-sidenav-content/div/div[2]/chat-window/div/input-container/div/input-area-v2/div/div/div[1]/div/div/rich-textarea/div[1]"
